  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ConfigMgr</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sites">
            <Value>=Parameters!sites.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT [t0].[Name] AS [name],
		[t1].[UserName0],
		[t1].[DisplayName0] AS [displayname],
		[t1].[Publisher0] AS [publisher],
		[t1].[DisplayVersion0] AS [version],
		[t1].[InstallDate0] AS [installdate],
		[t0].[SiteCode]
FROM [dbo].[v_FullCollectionMembership] AS [t0] 
	LEFT JOIN [dbo].[v_GS_USER_BASED_APPLICATIONS] AS [t1] ON [t0].[ResourceID] = [t1].[ResourceID] 
WHERE [t0].[CollectionID] = @CollectionID
	AND [t0].[SiteCode] IN (@sites)
	AND (ISNULL([t0].[IsObsolete], 0) &lt;&gt; 1) 
	AND (ISNULL([t0].[IsDecommissioned], 0) &lt;&gt; 1)
	AND [t0].[IsClient] = 1
	AND [t1].[DisplayName0] IS NOT NULL
    AND [t1].[DisplayName0] &lt;&gt; ''
ORDER BY [name], [displayname]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserName0">
          <DataField>UserName0</DataField>
